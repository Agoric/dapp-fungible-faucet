{
  "name": "@agoric/dapp-fungible-faucet-ui",
  "version": "0.0.1",
  "description": "dapp Frontend",
  "author": "Agoric",
  "license": "Apache-2.0",
  "homepage": ".",
  "type": "module",
  "scripts": {
    "build": "parcel build public/index.html",
    "lint-check": "eslint '**/*.{js,jsx}'",
    "lint-fix": "eslint --fix '**/*.{js,jsx}'",
    "start": "parcel serve --host=127.0.0.1 --port=3000 public/index.html",
    "test": "exit 0"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintIgnore": [
    "dist/"
  ],
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "public/lib",
        "staticOutDir": "lib"
      }
    ],
    "excludeGlob": "**/*.js",
    "watcherGlob": "**/*.html"
  },
  "dependencies": {
    "@agoric/notifier": "getting-started-dev",
    "@agoric/web-components": "getting-started-dev",
    "@endo/captp": "^2.0.3",
    "@endo/marshal": "^0.6.9",
    "lit": "^2.0.2",
    "regenerator-runtime": "0.13.7",
    "ses": "^0.18.8"
  },
  "devDependencies": {
    "caniuse-lite": "1.0.30001251",
    "parcel": "^2.10.0",
    "parcel-plugin-static-files-copy": "^2.5.0",
    "rimraf": "^3.0.2"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  }
}
